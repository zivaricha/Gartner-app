<h1>Hello world</h1>

<%= form_tag(profiles_path, :method => "get", id: "search-form") do %>
<%= text_field_tag :search, params[:search], placeholder: "Search Profiles" %><br>
<!--<%= text_field_tag :search, params[:search], placeholder: "Search Skills" %>-->
<%= submit_tag "Search" %>
<% end %>

<ul>
<% for profile in @profiles %>
  <li>
    <%= link_to profile.name, profile_path(profile) %>
    <%= link_to "Edit", edit_profile_path(profile) %>
    <%= link_to "Destroy", profile_path(profile), :method => :delete %>
  </li>
<% end %>
</ul>